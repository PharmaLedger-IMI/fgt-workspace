{
  "name": "pdm-web-components",
  "version": "0.2.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/pdm-web-components/pdm-web-components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "ionic:build": "npm run build",
    "build": "stencil build --dev --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "docs": "node ./node_modules/octopus/scripts/run docs"
  },
  "dependencies": {
    "@capacitor/app": "1.0.0",
    "@capacitor/camera": "^1.0.0",
    "@capacitor/core": "3.0.0",
    "@capacitor/haptics": "1.0.0",
    "@capacitor/keyboard": "1.0.0",
    "@ionic-native/barcode-scanner": "^5.33.0",
    "@ionic-native/core": "^5.33.0",
    "@zxing/library": "^0.18.5",
    "bwip-js": "^3.0.0",
    "phonegap-plugin-barcodescanner": "^8.1.0",
    "stencil": "0.0.5"
  },
  "license": "MIT",
  "devDependencies": {
    "@capacitor/cli": "3.0.0",
    "@ionic/cli": "^6.16.1",
    "@ionic/core": "^5.6.7",
    "@stencil/core": "^2.5.2",
    "jsdoc": "^3.6.6",
    "markdown-include": "^0.4.3",
    "octopus": "git+https://github.com/PrivateSky/octopus.git#1.0"
  }
}
