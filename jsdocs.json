{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [
            "fgt-dsu-wizard"
        ],
        "exclude": [
            "gtin-dsu-wizard",
            "gtin-resolver",
            "bin",
            "apihub-root",
            "cardinale",
            "privatesky",
            "node_modules",
            "build/bundles"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./docs/",
        "readme": "README.md",
        "recurse": true,
        "package": "package.json"
    }
}