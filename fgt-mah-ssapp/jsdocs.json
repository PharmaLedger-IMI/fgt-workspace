{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize"
    ],
    "package": "package.json",
    "recurseDepth": 10,
    "source": {
        "include": [
            "./code"
        ],
        "exclude": [
            "./code/scripts/bundles",
            "./code/constitution",
            "./code/resources",
            "./code/scripts/capacitor.js"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "better-docs": {
            "name": "Pharmaledger's Finished Goods Traceability Marketing Authorization Holder SSApp",
            "title": "Pharmaledger's Finished Goods Traceability Marketing Authorization Holder SSApp",
            "hideGenerator": true,
            "navLinks": [
                {
                    "label": "Github",
                    "href": "https://github.com/PharmaLedger-IMI/fgt-workspace/fgt-mah-ssapp.git"
                },
                {
                    "label": "Application",
                    "href": "https://fgt.pharmaledger.pdmfc.com/fgt-mah-wallet/loader"
                }
            ]
        },
        "cleverLinks": false,
        "monospaceLinks": false,
        "search": true
    },
    "opts": {
        "encoding": "utf-8",
        "template": "./node_modules/better-docs",
        "destination": "./docs/",
        "readme": "./README.md",
        "recurse": true,
        "verbose": true
    }
}